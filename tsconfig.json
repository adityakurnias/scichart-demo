{
  "include": [
    "./src/**/*",
    "./vite.config.ts"
  ],
"compilerOptions": {
  "strict": false,
  "strictPropertyInitialization": false,
  "esModuleInterop": true,
      "target": "es5",
    "moduleResolution": "node",
  "downlevelIteration": true,
  "lib": [
      "dom",
      "es2015"
  ],
  "typeRoots": ["./src/types", "./node_modules/@types"],
  "jsx": "react-jsx"
}
}